{
    "type":"origins:multiple",
    "number_of_jumps": {
      "type": "origins:resource",
      "min": 0,
      "max": 2,
      "hud_render": {
          "should_render": false
      }
    },
    "jump_reset": {
      "type": "origins:action_on_land",
      "entity_action":  {
        "type": "origins:change_resource",
          "resource": "*:*_number_of_jumps",
          "change": 2
    }
  },
    "activating_jump": {
      "type":"origins:active_self",
      "key": {
        "key":"key.jump"
      },
      "cooldown": 1,
      "hud_render": {
        "should_render": false
      },
      "entity_action": {
          "type": "origins:and",
          "actions": [
            {
              "type": "origins:add_velocity",
              "set": true,
              "y": 0.5
            },
            {
              "type": "origins:execute_command",
              "command": "particle minecraft:dust_color_transition 0 0.6 0.6 1 0 0.9 0.5 ~ ~ ~ 0.3 0 0.3 1 100",
              "permission_level": 4
            },
            {
              "type": "origins:execute_command",
              "command": "particle minecraft:glow ~ ~ ~ 0.2 0 0.2 0.01 100",
              "permission_level": 4
            },
            {
              "type": "origins:if_else",
              "condition": {
                "type": "origins:moving"
              },
              "if_action": {
                "type": "origins:add_velocity",
                "space":"local",
                "z": 0.2
              }
            },
            {
              "type": "origins:change_resource",
                "resource": "*:*_number_of_jumps",
                "change": -1
          }
        ]
      },
      "condition": {
        "type":"origins:and",
        "conditions":[
          {
          "type":"origins:on_block",
          "inverted": true
          },
          {
            "type": "origins:resource",
            "resource": "*:*_number_of_jumps",
            "comparison": ">",
            "compare_to": 0
          },
          {
            "type": "origins:fluid_height",
            "fluid": "minecraft:water",
            "comparison": "==",
            "compare_to": 0
          },
          {
            "type": "origins:fluid_height",
            "fluid": "minecraft:lava",
            "comparison": "==",
            "compare_to": 0
          },
          {
            "type": "origins:power",
            "power": "hypno:playstyle/hermes/ratatoskr/dysfunction",
            "inverted": true
          },
          {
            "type": "origins:power",
            "power": "hypno:playstyle/hermes/ratatoskr/portaling",
            "inverted": true
        }
        ]
      }
    },
    "name":"Branches of the World Tree",
    "description":"You can summon the branches of the World Tree to help you traverse the land. By using your [Jump Key] after you initial jump, you can jump for 2 additional times."
  }
  