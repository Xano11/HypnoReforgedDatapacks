{
    "name": "Disguised",
    "type": "origins:multiple",
    "teleport":{
        "type": "origins:action_over_time",
        "entity_action": {
            "type":"origins:and",
            "actions":[{
                "type": "origins:execute_command",
                "command": "execute as @e[tag=disguise_tp] if score @s disguised = @p[tag=!disguise_tp] disguised at @s run tp @p[tag=!disguise_tp] ~ ~ ~"
                    }
            ]
        },
        "interval": 1
    },
    "ignore":
    {
        "type": "apugli:mobs_ignore"
    },
    "invisible":
    {
        "type": "origins:invisibility",
        "render_armor": false
    },
    "no_block_interaction":
    {
        "type": "origins:prevent_block_selection"
    },
    "no_block_use":
    {
        "type": "origins:prevent_block_use"
    },
    "no_jump":{
        "type": "origins:modify_jump",
        "modifier": {
            "operation": "multiply_total",
            "value": -1
        }
    },
    "prevent_item_use":{
        "type": "origins:prevent_item_use"
    },
    "prevent_collision":{
        "type": "origins:prevent_entity_collision"
    },
    "no_damage_taken":
    {
        "type": "origins:modify_damage_taken",
        "modifier": {
            "name": "No damage",
            "operation": "multiply_total",
            "value": -1
        }
    },
    "no_damage_dealt":
    {
        "type": "origins:modify_damage_dealt",
        "modifier": {
            "name": "No damage",
            "operation": "multiply_total",
            "value": -1
        }
    },
    "no_momentum":{
        "type": "origins:modify_velocity",
        "modifier": {
          "value": -1,
          "operation": "multiply_total"
        },
        "axes": [
        "x",
        "y",
        "z"
        ]
      },
      "resource": {
        "type": "origins:resource",
        "min": 0,
        "max": 10,
        "start_value": 10,
        "hud_render": {
            "should_render": false
        },
        "min_action": {
            "type":"origins:and",
            "actions":[{
            "type": "origins:execute_command",
            "command": "power remove @s hypno:playstyle/hephaestus/tanuki/disguised"
                }
            ]
        }
      },
      "resource_down":{
        "type": "origins:action_over_time",
        "entity_action": {
            "type":"origins:and",
            "actions":[{
            "type": "origins:change_resource",
            "resource": "hypno:playstyle/hephaestus/tanuki/disguised_resource",
            "change": -1
                }
            ]
        },
        "interval": 1,
        "condition":{
            "type":"origins:and",
            "conditions":[{
            "type": "origins:sneaking"
                },
                {
                    "type": "origins:command",
                    "command": "execute as @e[distance=..2] if score @s disguised = @p disguised",
                    "comparison": "==",
                    "compare_to": 0
                }
            ]
        }
    },
    "resource_up":{
        "type": "origins:action_over_time",
        "entity_action":{
            "type": "origins:change_resource",
            "resource": "hypno:playstyle/hephaestus/tanuki/disguised_resource",
            "change": 10
        },
        "interval": 20,
        "condition":{
            "type":"origins:and",
            "conditions":[
                {
                    "type": "origins:command",
                    "command": "execute as @e[distance=..2] if score @s disguised = @p disguised",
                    "comparison": "==",
                    "compare_to": 1
                }
            ]
        }
    },
    "jump_out":{
        "type": "origins:active_self",
        "entity_action": {
            "type":"origins:and",
            "actions":[{
            "type": "origins:change_resource",
            "resource": "hypno:playstyle/hephaestus/tanuki/disguised_leave",
            "change": 1
        },
        {
            "type": "origins:execute_command",
            "command": "power remove @s hypno:playstyle/hephaestus/tanuki/disguised_no_momentum"
        }
    ]
        },
        "cooldown": 1,
        "hud_render": {
            "should_render": false
        },
        "key": {
            "key": "key.jump"
        }
    },
    "leave": {
        "type": "origins:resource",
        "min": 0,
        "max": 1,
        "start_value": 0,
        "hud_render": {
            "should_render": false
        },
        "max_action": {
            "type":"origins:and",
            "actions":[{
                "type": "origins:execute_command",
                "command": "execute as @e[tag=disguise_tp,limit=1,sort=nearest] run power remove @s hypno:playstyle/hephaestus/tanuki/disguise"
            },
            {
                "type": "origins:execute_command",
                "command": "execute as @e[tag=disguise_tp,limit=1,sort=nearest] run scoreboard players reset @s disguised"
            },
            {
                "type": "origins:execute_command",
                "command": "execute as @e[tag=disguise_tp,limit=1,sort=nearest] run tag @s remove disguise_tp"
            },
            {
                "type": "origins:execute_command",
                "command": "power remove @s hypno:playstyle/hephaestus/tanuki/disguised_no_momentum"
            },
            {
                "type":"origins:delay",
                "ticks":1,
                "action": {
                    "type": "origins:add_velocity",
                    "z": -0.5,
                    "space":"local"
                }
            },
            {
                "type":"origins:delay",
                "ticks":1,
                "action": {
                    "type": "origins:add_velocity",
                    "y": 0.5,
                    "space":"local"
                }
            },
            {
                "type":"origins:delay",
                "ticks":1,
                "action":{
                "type": "origins:execute_command",
                "command": "tag @s remove disguised"
                }
            },
            {
                "type":"origins:delay",
                "ticks":2,
                "action":{
                "type": "origins:execute_command",
                "command": "power remove @s hypno:playstyle/hephaestus/tanuki/disguised"
                }
            }
            ]
        }
      },
    "hidden":false
}