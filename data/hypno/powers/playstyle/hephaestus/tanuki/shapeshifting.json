            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:area_of_effect",
                    "radius": 2,
                    "bientity_action": {
                        "type": "origins:target_action",
                        "action": {
                            "type": "origins:add_velocity",
                            "z": 0.08,
                            "space": "local_horizontal"
                        }
                    },
                    "bientity_condition": {
                        "type": "origins:and",
                        "conditions": [
                            {
                                "type": "eggolib:compare_score",
                                "actor_objective": "shapeshift",
                                "target_objective": "shapeshift",
                                "comparison": "=="
                            },
                            {
                                "type": "origins:target_condition",
                                "condition": {
                                    "type": "origins:and",
                                    "conditions": [
                                        {
                                            "type": "origins:in_tag",
                                            "tag": "hypno:living"
