                },
                {
                    "type": "origins:execute_command",
                    "command": "execute as @e[tag=shapeshift] if score @s shapeshift = @p shapeshift at @s run kill @s"
                },
                {
                    "type": "origins:execute_command",
                    "command": "scale reset @s"
