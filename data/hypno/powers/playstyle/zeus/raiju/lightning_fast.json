{
    "name":"Thunderous",
    "description":"You can toggle your Thunderous form using your [SECONDARY ABILITY KEY]. In this form, you are reduced to a ball of electricity allowing you to teleport short distances. Using this form costs energy over time.",
    "type": "origins:multiple",
    "toggle":{
        "type": "origins:toggle",
        "active_by_default": false,
        "key": {
            "key": "key.origins.secondary_active"
        },
        "condition":
        {
            "type": "origins:and",
            "conditions": [{
            "type": "origins:power_active",
            "power": "hypno:playstyle/zeus/raiju/discharged",
            "inverted": true
        },
        {
            "type": "origins:resource",
            "resource": "hypno:playstyle/zeus/raiju/energetic_resource",
            "comparison": ">=",
            "compare_to": 150
        }
        ]
    }
    },
    "resource_use":{
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:change_resource",
                    "resource": "hypno:playstyle/zeus/raiju/energetic_resource",
                    "change": -100
                }
            ]
        },
        "interval": 15,
        "condition": {
            "type": "origins:and",
            "conditions": [{
            "type": "origins:power_active",
            "power": "hypno:playstyle/zeus/raiju/lightning_fast_toggle"
        },
        {
            "type": "origins:resource",
            "resource": "hypno:playstyle/zeus/raiju/energetic_resource",
            "comparison": ">",
            "compare_to": 0
        }
        ]
    }
    },
    "particles":{
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "execute at @s run particle dust_color_transition 0.54 0.99 1 1 1 0.95 0.24 ~ ~1 ~ 0.1 0.1 0.1 0.1 5"
                }
            ]
        },     
        "interval": 1,
        "condition": {
            "type": "origins:and",
            "conditions": [{
            "type": "origins:power_active",
            "power": "hypno:playstyle/zeus/raiju/lightning_fast_toggle"
        },
        {
            "type": "origins:resource",
            "resource": "hypno:playstyle/zeus/raiju/energetic_resource",
            "comparison": ">",
            "compare_to": 0
        }
        ]
    }
    },
    "invisibility":{
        "type": "origins:invisibility",
        "render_armor": false,
        "condition":  {
            "type": "origins:and",
            "conditions": [{
            "type": "origins:power_active",
            "power": "hypno:playstyle/zeus/raiju/lightning_fast_toggle"
        },
        {
            "type": "origins:resource",
            "resource": "hypno:playstyle/zeus/raiju/energetic_resource",
            "comparison": ">",
            "compare_to": 0
        }
        ]
    }
},
    "no_move":
    {
        "type": "origins:modify_velocity",
        "modifier": {
          "value": -1,
          "operation": "multiply_total"
        },
        "axes": [
          "x",
          "y",
          "z"
        ],
        "condition":
        {
            "type": "origins:and",
            "conditions": [{
            "type": "origins:power_active",
            "power": "hypno:playstyle/zeus/raiju/lightning_fast_toggle"
