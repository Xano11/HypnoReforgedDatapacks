{
    "name": "Thunderous ◯ ● ◯",
    "description": "You can toggle your Thunderous form using your [SECONDARY ABILITY KEY]. In this form, you are reduced to a ball of electricity allowing you to teleport short distances. Using this form costs energy over time.",
    "type": "origins:multiple",
    "toggle": {
        "type": "origins:toggle",
        "active_by_default": false,
        "retain_state": false,
        "key": {
            "key": "key.origins.secondary_active"
        },
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:power",
                    "power": "hypno:playstyle/zeus/raiju/discharged",
                    "inverted": true
                },
                {
                    "type": "origins:power_active",
                    "power": "hypno:playstyle/zeus/raiju/stormfield_toggle",
                    "inverted": true
                },
                {
                    "type": "origins:resource",
                    "resource": "hypno:playstyle/zeus/raiju/energetic_resource",
                    "comparison": ">=",
                    "compare_to": 50
                },
                {
                    "type": "origins:power",
                    "power": "hypno:playstyle/zeus/raiju/emp",  
                    "inverted": true
                },
                {
                    "type": "origins:resource",
                    "resource": "hypno:playstyle/zeus/raiju/lightning_orb_resource",
                    "comparison": "<",
                    "compare_to": 50
                }
            ]
        }
    },
    "particles": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "execute at @s run particle dust_color_transition 0.54 0.99 1 1 1 0.95 0.24 ~ ~1 ~ 0.1 0.1 0.1 0.1 5"
                }
            ]
        },
        "interval": 1,
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:power_active",
                    "power": "hypno:playstyle/zeus/raiju/lightning_fast_toggle"
                },
                {
                    "type": "origins:resource",
                    "resource": "hypno:playstyle/zeus/raiju/energetic_resource",
                    "comparison": ">",
                    "compare_to": 0
                }
            ]
        }
    },
    "invisibility": {
        "type": "origins:invisibility",
        "render_armor": false,
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:power_active",
                    "power": "hypno:playstyle/zeus/raiju/lightning_fast_toggle"
                },
                {
                    "type": "origins:resource",
                    "resource": "hypno:playstyle/zeus/raiju/energetic_resource",
                    "comparison": ">",
                    "compare_to": 0
                }
            ]
        }
    },
    "no_move": {
        "type": "origins:modify_velocity",
        "modifier": {
            "value": -1,
            "operation": "multiply_total"
        },
        "axes": [
            "x",
            "y",
            "z"
        ],
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:power_active",
                    "power": "hypno:playstyle/zeus/raiju/lightning_fast_toggle"
                },
                {
                    "type": "origins:resource",
                    "resource": "hypno:playstyle/zeus/raiju/energetic_resource",
                    "comparison": ">",
                    "compare_to": 0
                }
            ]
        }
    },
    "move": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:raycast",
            "distance": 8,
            "block": true,
            "entity": true,
            "shape_type": "visual",
            "fluid_handling": "any",
            "before_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:execute_command",
                        "command": "playsound simplyswords:elemental_sword_thunder_attack_01 player @a[distance=..16] ~ ~ ~ 1 2"
                    },
                    {
                        "type": "origins:execute_command",
                        "command": "playsound minecraft:block.respawn_anchor.charge player @a ~ ~ ~ 1 2"
                    },
                    {
                        "type": "origins:change_resource",
                        "resource": "hypno:playstyle/zeus/raiju/energetic_resource",
                        "change": -50
                    },
                    {
                        "type": "origins:add_velocity",
                        "y": 0,
                        "set": true
                    },
                    {
                        "type": "origins:set_fall_distance",
                        "fall_distance": 0
                    }
                ]
            },
            "command_at_hit": "function hypno:lightninghit",
            "command_along_ray": "function hypno:lightningtp",
            "command_step": 0.2,
            "command_along_ray_only_on_hit": false
        },
        "cooldown": 15,
        "key": {
            "key": "key.sprint",
            "continuous": true
        },
        "hud_render": {
            "should_render": false
        },
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:power_active",
                    "power": "hypno:playstyle/zeus/raiju/lightning_fast_toggle"
                },
                {
                    "type": "origins:resource",
                    "resource": "hypno:playstyle/zeus/raiju/energetic_resource",
                    "comparison": ">",
                    "compare_to": 0
                }
            ]
        }
    },
    "emp": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "power grant @s hypno:playstyle/zeus/raiju/emp"
                },
                {
                    "type": "origins:toggle",
                    "power": "hypno:playstyle/zeus/raiju/lightning_fast_toggle"
                }
            ]
        },
        "cooldown": 30,
        "key": {
            "key": "key.origins.primary_active"
        },
        "hud_render": {
            "should_render": false
        },
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:power_active",
