    }
      },
    "move":{
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:raycast",
            "distance": 8,
            "block": true,
            "entity": true,
            "shape_type": "visual",
            "fluid_handling": "any",
            "before_action": {
                "type": "origins:and",
                "actions": [{
                "type": "origins:execute_command",
                "command": "playsound soulsweapons:gatling_gun_startup player @s ~ ~ ~ 0.5 2"
                    },
                    {
                "type": "origins:execute_command",
                "command": "playsound minecraft:block.respawn_anchor.charge player @s ~ ~ ~ 0.5 2"
                    }
                ]
            },
            "command_at_hit": "summon minecraft:lightning_bolt",
            "command_along_ray": "function hypno:lightningtp",
            "command_step": 0.2,
            "command_along_ray_only_on_hit": false
        },
        "cooldown": 15,
        "key": {
            "key": "key.sprint",
            "continuous": true
        },
        "hud_render": {
            "should_render": false
        },
        "condition":
        {
            "type": "origins:and",
