{
    "name":"Stunned",
    "description":"You're stunned and can no move.",
    "type": "origins:multiple",
    "res":{
        "type": "origins:resource",
        "min": 1,
        "max": 2,
        "start_value": 1,
        "hud_render": {
            "should_render": false
        }
    },

	"res_adjust":{
    	"type": "origins:self_action_when_hit",
    	"entity_action": {
			"type": "origins:and",
			"actions": [
				{
					"type": "origins:change_resource",
    				"resource": "*:*_res",
   					"change": 1
				},
				{    
					"type": "origins:add_velocity",
                    "y": -100,
                    "space": "local_horizontal"
				}
			]
		}
    },

	"no_move": {
		"type": "origins:attribute",
		"modifier": {
			"name": "Decrease speed",
			"attribute": "minecraft:generic.movement_speed",
			"value": -1,
			"operation": "multiply_total"
		},
		"condition": {
			"type": "origins:resource",
			"resource": "*:*_res",
			"comparison": "==",
			"compare_to": 2
		}
	},

    "no_atk_speed": {
		"type": "origins:attribute",
		"modifier": {
			"name": "Decrease speed",
			"attribute": "minecraft:generic.attack_speed",
			"value": -0.6,
			"operation": "multiply_total"
		},
		"condition": {
			"type": "origins:resource",
			"resource": "*:*_res",
			"comparison": "==",
			"compare_to": 2
		}
	},

	"no_jump": {
		"type": "origins:modify_jump",
		"modifier": {
			"operation": "multiply_total",
			"value": -1
		},
		"condition": {
			"type": "origins:resource",
			"resource": "*:*_res",
			"comparison": "==",
			"compare_to": 2
		}
	},

	"no_elytra":{
		"type": "origins:prevent_elytra_flight",
		"condition": {
			"type": "origins:resource",
			"resource": "*:*_res",
			"comparison": "==",
			"compare_to": 2
		}
	},

	"no_attack": {
		"type": "origins:modify_damage_dealt",
		"modifier": {
			"operation": "multiply_total",
			"value": -0.99999999999
		},
		"condition": {
			"type": "origins:resource",
			"resource": "*:*_res",
			"comparison": "==",
			"compare_to": 2
		}
	},

	"no_air": {
		"type": "origins:modify_air_speed",
		"modifier": {
			"operation": "multiply_total",
			"value": -1
		},
		"condition": {
			"type": "origins:resource",
			"resource": "*:*_res",
			"comparison": "==",
			"compare_to": 2
		}
	},
	"ignore_water":{
		"type": "origins:ignore_water",
		"condition": {
			"type": "origins:resource",
			"resource": "*:*_res",
			"comparison": "==",
			"compare_to": 2
		}
			},

    "lose":{
        "type": "origins:action_on_land",
        "entity_action": {
            "type": "origins:if_else",
    		"condition": {
        		"type": "origins:resource",
    			"resource": "*:*_res",
    			"comparison": "==",
    			"compare_to": 1
    		},
    		"if_action": {
        		"type": "origins:execute_command",
                "command": "power remove @s hypno:playstyle/zeus/minotaur/cucked"
    		},
    		"else_action": {
        		"type": "origins:and",
    			"actions": [
       			{
            		"type": "origins:damage",
                      "amount": 1,
                      "source": {
                          "name": "cramming.player",
                          "bypasses_armor": true,
						  "unblockable": true
                      }
        		},
        		{    
            		"type":"origins:delay",
                    "ticks":15,
                    "action":
                    {
                    "type": "origins:execute_command",
                    "command": "power remove @s hypno:playstyle/zeus/minotaur/cucked"
                    }
        		}
    		]
		}
	}  
    },
    "hidden": true
}