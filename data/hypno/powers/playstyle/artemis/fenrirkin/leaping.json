{
    "name": "Leaping",
    "type": "origins:multiple",
    "resource": {
        "type": "origins:resource",
        "min": 0,
        "max": 60,
        "start_value": 60,
        "hud_render": {
            "sprite_location": "origins:textures/gui/resource_bar.png",
            "bar_index": 2
        },
        "min_action": {
            "type": "origins:execute_command",
            "command": "power remove @s hypno:playstyle/artemis/fenrirkin/leaping"
        }
    },
    "particles": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "particle minecraft:snowflake ~ ~1 ~ 0.5 0.5 0.5 0.1 10"
                }
            ]
        },
        "interval": 2
    },
    "resource_use": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:change_resource",
                    "resource": "hypno:playstyle/artemis/fenrirkin/leaping_resource",
                    "change": -1
                },
                {
                    "type": "origins:raycast",
                    "distance": 32,
                    "block": true,
                    "entity": true,
                    "shape_type": "collider",
                    "fluid_handling": "any",
                    "command_at_hit": "particle minecraft:dust_color_transition 0 1 1 1 1 1 1 ~ ~ ~ 0.3 0.1 0.3 0 10",
                    "command_step": 0.2,
                    "command_along_ray_only_on_hit": true
                }
            ]
        },
        "interval": 1
    },
    "no_movement": {
        "type": "origins:modify_velocity",
        "modifier": {
            "value": -1,
            "operation": "multiply_total"
        },
        "axes": [
            "x",
            "y",
            "z"
        ]
    },
    "no_sprint": {
        "type": "origins:prevent_sprinting"
    },
    "no_collision": {
        "type": "origins:prevent_entity_collision"
    },
    "no_block_interaction": {
        "type": "origins:prevent_block_selection"
    },
    "no_block_use": {
        "type": "origins:prevent_block_use"
    },
    "prevent_item_use": {
        "type": "origins:prevent_item_use"
    },
    "overlay": {
        "type": "origins:overlay",
        "texture": "minecraft:textures/block/ice.png",
        "strength": 0.5,
        "red": 1.0,
        "green": 1.0,
        "blue": 1.0,
        "draw_mode": "texture",
        "draw_phase": "below_hud",
        "hide_with_hud": false,
        "visible_in_third_person": true
    },
    "leap": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:raycast",
            "distance": 32,
            "block": true,
            "entity": true,
            "shape_type": "collider",
            "fluid_handling": "any",
            "hit_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:area_of_effect",
                        "radius": 5,
                        "bientity_action": {
                            "type": "origins:and",
                            "actions": [
                                {
                                    "type": "origins:damage",
                                    "amount": 5,
                                    "source": {
                                        "name": "freeze.player",
                                        "bypasses_armor": true
                                    }
                                },
                                {
                                    "type": "origins:target_action",
                                    "action": {
                                        "type": "origins:and",
                                        "actions": [
                                            {
                                                "type": "origins:change_resource",
                                                "resource": "hypno:playstyle/artemis/fenrirkin/freezing_stacks",
                                                "change": 3
                                            },
                                            {
                                                "type": "origins:execute_command",
                                                "command": "particle minecraft:block minecraft:blue_ice ~ ~1 ~ 0.5 0.5 0.5 1 10"
                                            },
                                            {
                                                "type": "origins:execute_command",
                                                "command": "power grant @s hypno:playstyle/artemis/fenrirkin/freezing"
                                            },
                                            {
                                                "type": "origins:execute_command",
                                                "command": "tag @s add freezing"
                                            },
                                            {
                                                "type": "origins:change_resource",
                                                "resource": "hypno:playstyle/artemis/fenrirkin/freezing_timer",
                                                "change": 25
                                            }
                                        ]
                                    }
                                }
                            ]
                        },
                        "bientity_condition": {
                            "type": "origins:target_condition",
                            "condition": {
                                "type": "origins:and",
                                "conditions": [
                                    {
                                        "type": "origins:living"
                                    },
                                    {
                                        "type": "origins:in_tag",
                                        "tag": "hypno:living"
                                    },
                                    {
                                        "type": "origins:origin",
                                        "origin": "hypno:playstyle/artemis/fenrirkin",
                                        "layer": "origins:artemis",
                                        "inverted": true
                                    }
                                ]
                            }
                        }
                    },
                    {
                        "type": "origins:delay",
                        "ticks": 2,
                        "action": {
                            "type": "origins:execute_command",
                            "command": "power remove @s hypno:playstyle/artemis/fenrirkin/leaping"
                        }
                    }
                ]
            },
            "before_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:execute_command",
                        "command": "playsound simplyswords:elemental_bow_ice_shoot_impact_01 player @a[distance=..16] ~ ~ ~ 1 0.5"
                    },
                    {
                        "type": "origins:execute_command",
                        "command": "playsound simplyswords:elemental_bow_ice_shoot_flyby_02 player @a[distance=..16] ~ ~ ~ 1 0.5"
                    }
                ]
            },
            "block_action":{
                "type": "eggolib:area_of_effect",
                "radius": 3,
                "shape": "star",
                "block_action": {
                    "type": "apoli:set_block",
                    "block": "minecraft:frosted_ice"
                },
                "block_condition":  {
                    "type": "origins:block",
                    "block": "minecraft:water"
                }
            },
            "command_along_ray": "function hypno:leaping",
            "command_step": 0.2,
            "command_along_ray_only_on_hit": true
        },
        "cooldown": 10,
        "key": {
            "key": "key.use",
            "continuous": false
        },
        "hud_render": {
            "should_render": false
        }
    },
    "hidden": true
}