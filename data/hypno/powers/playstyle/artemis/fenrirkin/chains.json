{
  "name": "Break free",
  "description": "Eternal chains bind you down, but you can break free of them and go rampant. Using [PRIMARY ABILITY KEY], you can go into your Unchained form, granting you extra speed. But, being in this form consumes alot of hunger and makes you more reckless (Less invincibility frames and more damage taken). Hitting an entity will replenish some food whilst unchained.",
  "type": "origins:multiple",
  "toggle": {
    "type": "origins:toggle",
    "active_by_default": false,
    "key": {
      "key": "key.origins.primary_active"
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:power",
          "power": "hypno:playstyle/artemis/fenrirkin/wild",
          "inverted": true
        },
        {
          "type": "origins:resource",
          "resource": "hypno:playstyle/artemis/fenrirkin/chains_timer",
          "comparison": "==",
          "compare_to": 0
        },
        {
          "type": "origins:resource",
          "resource": "hypno:playstyle/artemis/fenrirkin/chains_timer_unchained",
          "comparison": "==",
          "compare_to": 0
        }
      ]
    }
  },
  "toggle_activate": {
    "type": "origins:active_self",
    "entity_action": {
      "type": "origins:if_else",
      "condition":  {
        "type": "origins:power_active",
        "power": "hypno:playstyle/artemis/fenrirkin/chains_toggle",
        "inverted": true
      },
      "if_action": {
        "type": "origins:execute_command",
        "command": "resource set @s hypno:playstyle/artemis/fenrirkin/chains_timer_unchained 30"
      },
      "else_action": {
        "type":"origins:and",
        "actions":[{
          "type": "origins:execute_command",
          "command": "resource set @s hypno:playstyle/artemis/fenrirkin/chains_timer 10"
        },
        {
          "type": "origins:execute_command",
          "command": "summon armor_stand ~ ~ ~ {Invisible:1b,Invulnerable:1b,NoBasePlate:1b,NoGravity:1b,Marker:1b,Tags:[\"chained\"]}"
        },
        {
          "type": "origins:execute_command",
          "command": "summon armor_stand ~ ~ ~ {Invisible:1b,Invulnerable:1b,NoBasePlate:1b,NoGravity:1b,Marker:1b,Tags:[\"chained\"]}"
        },
        {
          "type": "origins:execute_command",
          "command": "summon armor_stand ~ ~ ~ {Invisible:1b,Invulnerable:1b,NoBasePlate:1b,NoGravity:1b,Marker:1b,Tags:[\"chained\"]}"
        },
        {
          "type": "origins:execute_command",
          "command": "summon armor_stand ~ ~ ~ {Invisible:1b,Invulnerable:1b,NoBasePlate:1b,NoGravity:1b,Marker:1b,Tags:[\"chained\"]}"
        },
        {
          "type": "origins:execute_command",
          "command": "summon armor_stand ~ ~ ~ {Invisible:1b,Invulnerable:1b,NoBasePlate:1b,NoGravity:1b,Marker:1b,Tags:[\"chained\"]}"
        },
        {
          "type": "origins:execute_command",
          "command": "summon armor_stand ~ ~ ~ {Invisible:1b,Invulnerable:1b,NoBasePlate:1b,NoGravity:1b,Marker:1b,Tags:[\"chained\"]}"
        },
        {
          "type": "origins:execute_command",
          "command": "summon armor_stand ~ ~ ~ {Invisible:1b,Invulnerable:1b,NoBasePlate:1b,NoGravity:1b,Marker:1b,Tags:[\"chained\"]}"
        },
        {
          "type": "origins:execute_command",
          "command": "summon armor_stand ~ ~ ~ {Invisible:1b,Invulnerable:1b,NoBasePlate:1b,NoGravity:1b,Marker:1b,Tags:[\"chained\"]}"
        },
        {
          "type": "origins:delay",
          "ticks": 1,
          "action": {
            "type": "origins:and",
            "actions": [
              {
                "type": "origins:execute_command",
                "command": "spreadplayers ~ ~ 1 4 false @e[type=minecraft:armor_stand,tag=chained]"
              },
              {
                "type": "origins:execute_command",
                "command": "tag @s add chain_particles"
              }
            ]
          }
        },
        {
          "type": "origins:delay",
          "ticks": 2,
          "action": {
            "type": "origins:and",
            "actions": [
              {
                "type": "origins:execute_command",
                "command": "execute as @e[type=minecraft:armor_stand,tag=chained] at @s anchored eyes facing entity @p[tag=chain_particles] eyes run tp @s ~ ~ ~ ~ ~"
              },
              {
                "type": "origins:execute_command",
                "command": "execute as @e[type=minecraft:armor_stand,tag=chained] run power grant @s hypno:playstyle/artemis/fenrirkin/particles_chain"
              }
            ]
          }
        },
        {
          "type": "origins:delay",
          "ticks": 3,
          "action": {
            "type": "origins:and",
            "actions": [
              {
                "type": "origins:execute_command",
                "command": "execute positioned ~ ~1 ~ run function hypno:chains"
              },
              {
                "type": "origins:execute_command",
                "command": "tag @s remove chain_particles"
              }
            ]
          }
        }
      ]
      }
    },
