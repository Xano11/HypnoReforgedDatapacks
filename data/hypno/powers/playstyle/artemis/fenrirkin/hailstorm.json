{
  "name": "Hailstorm",
  "description": "You've acquired Freyr's sword and can use it to unleash Hailstorm. Pressing your [Secondary Ability Key] whilst chained will plant the legendary sword into the ground, creating a hailstorm. In the hailstorm, icicles fall; dealing damage and freezing entities over time. Whilst Unchained, you can channel the energy of the sword to Leap. Leaping will launch you in the air, and stop you, letting you choose your landing area (Press your [USE] keybind).",
  "type": "origins:multiple",
  "activate_sword": {
    "type": "origins:active_self",
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "summon armor_stand ~ ~ ~ {Invisible:1b,Invulnerable:1b,Marker:1b,NoBasePlate:1b,ShowArms:1b,ArmorItems:[{},{},{},{}],HandItems:[{id:\"soulsweapons:freyr_sword\",Count:1b},{}],Tags:[\"hailstorm\"],Pose:{RightArm:[90f,0f,0f]}}"
        },
        {
          "type": "origins:execute_command",
          "command": "playsound soulsweapons:sharpen player @a ~ ~ ~ 2 0.6"
        },
        {
          "type": "origins:execute_command",
          "command": "playsound soulsweapons:shield player @a ~ ~ ~ 0.5 0.7"
        },
        {
          "type": "origins:execute_command",
          "command": "particle minecraft:snowflake ~ ~ ~ 0.1 0.1 0.1 1 100"
        },
        {
          "type": "origins:execute_command",
          "command": "tag @s add hailstorm_blade"
        },
        {
          "type": "origins:execute_command",
          "command": "execute store result score @s hailstorm_blade run data get entity @s UUID[0] 1"
        },
        {
          "type": "origins:delay",
          "ticks": 1,
          "action": {
            "type": "origins:execute_command",
            "command": "power grant @e[limit=1,sort=nearest,tag=hailstorm] hypno:playstyle/artemis/fenrirkin/blade"
          }
        },
        {
          "type": "origins:delay",
          "ticks": 1,
          "action": {
            "type": "origins:execute_command",
            "command": "execute store result score @e[limit=1,sort=nearest,type=armor_stand,tag=hailstorm] hailstorm_blade run data get entity @p[tag=hailstorm_blade] UUID[0] 1"
          }
        },
        {
          "type": "origins:delay",
          "ticks": 2,
          "action": {
            "type": "origins:execute_command",
            "command": "tag @s remove hailstorm"
          }
        }
      ]
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:on_block"
        },
        {
          "type": "origins:power_active",
          "power": "hypno:playstyle/artemis/fenrirkin/chains_toggle",
          "inverted": true
        }
      ]
    },
    "hud_render": {
      "should_render": true,
      "sprite_location": "origins:textures/gui/community/spiderkolo/resource_bar_03.png",
      "bar_index": 0
    },
    "key": {
      "key": "key.origins.secondary_active"
    },
    "cooldown": 900
  },
  "pickup_sword": {
