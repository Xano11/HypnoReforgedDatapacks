{
    "type": "origins:multiple",
    "timer": {
        "type": "origins:resource",
        "min": 0,
        "max": 600,
        "start_value": 600,
        "hud_render": {
            "should_render": false
        },
        "min_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "kill @s"
                }
            ]
        }
    },
    "icicles": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "summon armor_stand ~ ~ ~ {Invisible:1b,Invulnerable:1b,NoBasePlate:1b,ArmorItems:[{},{},{},{}],HandItems:[{},{}],Tags:[\"icicle\"],DisabledSlots:4144959}"
                },
                {
                    "type": "origins:execute_command",
                    "command": "spreadplayers ~ ~ 2 8 false @e[type=minecraft:armor_stand,tag=icicle]"
                },
                {
                    "type": "origins:delay",
                    "ticks": 1,
                    "action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:execute_command",
                                "command": "execute as @e[tag=icicle] at @s run tp @s ~ ~7 ~"
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "power grant @e[tag=icicle] hypno:playstyle/artemis/fenrirkin/icicle"
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "tag @e[tag=icicle] remove icicle"
                            }
                        ]
                    }
                }
            ]
        },
        "interval": 7,
        "condition":{
            "type": "apugli:entity_in_radius",
            "bientity_condition": {
                "type": "origins:and",
                "conditions": [
                    {
                        "type": "eggolib:compare_score",
                        "actor_objective": "hailstorm_blade",
                        "target_objective": "hailstorm_blade",
                        "comparison": "!="
                    },
                    {
                        "type": "origins:target_condition",
                        "condition": {
                            "type": "origins:and",
                            "conditions": [
                                {
                                    "type": "origins:living"
                                }
                            ]
                        }
                    }
                ]
            },
            "radius": 9.0,
            "comparison": ">=",
            "compare_to": 1
        }
    },
    "timer_down": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:change_resource",
                    "resource": "hypno:playstyle/artemis/fenrirkin/blade_timer",
                    "change": -20
                },
                {
                    "type": "origins:execute_command",
                    "command": "particle minecraft:dust_color_transition 0 0.5 1 1 0 0.7 1 ~-0.45 ~ ~-0.4 0.1 0.2 0.1 0.1 20"
                },
                {
                    "type": "origins:execute_command",
                    "command": "function hypno:hailstorm"
                }
            ]
        },
        "interval": 20
    }
}