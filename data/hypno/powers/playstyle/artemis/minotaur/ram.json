{
  "name": "Ram",
  "description": "You can trigger a ram ability with your [PRIMARY ABILITY KEY], launching you forward at high speeds, Increasing your damage by 3 and launching anyone you hit.",
  "type": "origins:multiple",

  "trigger": {
    "type": "origins:active_self",
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:add_velocity",
          "z": 1,
          "space": "local_horizontal"
        },
        {
          "type": "origins:play_sound",
          "sound": "minecraft:entity.goat.long_jump"
        },
        {
          "type": "origins:change_resource",
          "resource": "*:*_remaining",
          "change": 40,
          "operation": "set"
        }
      ]
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:on_block"
        },
        {
          "type": "origins:resource",
          "resource": "*:*_remaining",
          "comparison": "==",
          "compare_to": 0
        }
      ]
    },
    "hud_render": {
      "should_render": false
    },
    "key": {
      "key": "key.origins.primary_active"
    },
    "cooldown": 200
  },

  "remaining": {
    "type": "origins:resource",
    "min": 0,
    "max": 40,
    "hud_render": {
      "should_render": true,
      "sprite_location": "horigins:textures/resource_bar01.png",
      "bar_index": 1,
      "condition": {
        "type": "origins:resource",
        "resource": "*:*_remaining",
        "comparison": ">",
        "compare_to": 0
      }
    }
  },

  "cooldown": {
    "type": "origins:resource",
    "min": 0,
    "max": 200,
    "hud_render": {
      "should_render": true,
      "sprite_location": "horigins:textures/resource_bar01.png",
      "bar_index": 0,
      "condition": {
        "type": "origins:and",
        "conditions": [
          {
            "type": "origins:resource",
            "resource": "*:*_cooldown",
            "comparison": ">",
            "compare_to": 0
          },
          {
            "type": "origins:resource",
            "resource": "*:*_cooldown",
            "comparison": "<",
            "compare_to": 200
          }
        ]
      }
    }
  },

  "run_effects": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:if_else",
      "condition": {
        "type": "origins:resource",
        "resource": "*:*_remaining",
        "comparison": ">",
        "compare_to": 0
      },
      "if_action": {
        "type": "origins:and",
          "actions": [
            {
              "type": "origins:execute_command",
              "command": "particle twigs:item_pebble ~ ~ ~ 0.4 0 0.4 10 50"
            },
            {
              "type": "origins:execute_command",
              "command": "particle create:steam_jet 0.2 ~ ~ ~ 0.25 0 0.25 1 6"
            },
            {
              "type": "origins:play_sound",
              "sound": "minecraft:entity.goat.step"
            }
          ]
        }
      },
    "interval": 3
  },

  "run_out": {
    "type": "origins:action_over_time",
    "rising_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:change_resource",
          "resource": "*:*_cooldown",
          "change": 0,
          "operation": "set"
        },
        {
          "type": "origins:trigger_cooldown",
          "power": "*:*_trigger"
        }
      ]
    },
    "interval": 1,
    "condition": {
      "type": "origins:resource",
      "resource": "*:*_remaining",
      "comparison": "==",
      "compare_to": 0
    }
  },

  "tick_down": {
    "type": "origins:action_over_time",
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:change_resource",
          "resource": "*:*_remaining",
          "change": -1,
          "operation": "add"
        },
        {
          "type": "origins:change_resource",
          "resource": "*:*_cooldown",
          "change": 1,
          "operation": "add"
        },
        {
          "type": "origins:if_else",
          "condition": {
            "type": "origins:resource",
            "resource": "*:*_remaining",
            "comparison": ">",
            "compare_to": 0
          },
          "if_action": {
            "type": "origins:trigger_cooldown",
            "power": "*:*_trigger"
          }
        },
        {
          "type": "origins:if_else",
          "condition": {
            "type": "origins:and",
            "conditions": [
              {
                "type": "origins:resource",
                "resource": "*:*_remaining",
                "comparison": ">",
                "compare_to": 0
              },
              {
                "type": "origins:resource",
                "resource": "*:*_remaining",
                "comparison": "<",
                "compare_to": 35
              },
              {
                "type": "origins:moving",
                "inverted": true
              }
            ]
          },
          "if_action": {
            "type": "origins:and",
            "actions": [
              {
                "type": "origins:damage",
                "source": {
                  "name": "flyIntoWall"
                },
                "amount": 6
              },
              {
                "type": "origins:change_resource",
                "resource": "*:*_remaining",
                "change": 0,
                "operation": "set"
              },
              {
                "type": "origins:play_sound",
                "sound": "minecraft:entity.goat.horn_break"
              }
            ]
          }
        }
      ]
    },
    "interval": 1
  },

  "movement_speed": {
    "type": "origins:conditioned_attribute",
    "modifier": {
      "attribute": "minecraft:generic.movement_speed",
      "operation": "multiply_total",
      "value": 0
    },
    "tick_rate": 5,
    "condition": {
      "type": "origins:resource",
      "resource": "*:*_remaining",
      "comparison": ">",
      "compare_to": 0
    }
  },

  "slipperiness": {
    "type": "origins:modify_slipperiness",
    "modifier": {
      "operation": "multiply_total",
      "value": 0.85
    },
    "condition": {
      "type": "origins:resource",
      "resource": "*:*_remaining",
      "comparison": ">",
      "compare_to": 0
    }
  },

  "sprinting": {
    "type": "origins:prevent_sprinting",
    "condition": {
      "type": "origins:resource",
      "resource": "*:*_remaining",
      "comparison": ">",
      "compare_to": 0
    }
  },

  "sleep": {
    "type": "origins:prevent_sleep",
    "message": "CHARGE NOW, THINK LATER",
    "condition": {
      "type": "origins:resource",
      "resource": "*:*_remaining",
      "comparison": ">",
      "compare_to": 0
    }
  },

  "item_use": {
    "type": "origins:prevent_item_use",
    "condition": {
      "type": "origins:resource",
      "resource": "*:*_remaining",
      "comparison": ">",
      "compare_to": 0
    }
  },

  "jump": {
    "type": "origins:modify_jump",
    "modifier": {
      "operation": "addition",
      "value": -100
    },
    "entity_action": {
      "type": "origins:execute_command",
      "command": "title @s actionbar {\"text\": \"CHARGE NOW, THINK LATER\", \"color\": \"white\"}"
    },
    "condition": {
      "type": "origins:resource",
      "resource": "*:*_remaining",
      "comparison": ">",
      "compare_to": 0
    }
  },

  "interact": {
    "type": "origins:prevent_entity_use",
    "bientity_action": {
      "type": "origins:actor_action",
      "action": {
        "type": "origins:execute_command",
        "command": "title @s actionbar {\"text\": \"CHARGE NOW, THINK LATER\", \"color\": \"white\"}"
      }
    },
    "condition": {
      "type": "origins:resource",
      "resource": "*:*_remaining",
      "comparison": ">",
      "compare_to": 0
    }
  },

  "elytra_flight": {
    "type": "origins:prevent_elytra_flight",
    "entity_action": {
      "type": "origins:execute_command",
      "command": "title @s actionbar {\"text\": \"CHARGE NOW, THINK LATER\", \"color\": \"white\"}"
    },
    "condition": {
      "type": "origins:resource",
      "resource": "*:*_remaining",
      "comparison": ">",
      "compare_to": 0
    }
  },

  "attack": {
    "type": "origins:action_on_hit",
    "bientity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:add_velocity",
          "y": 1,
          "space": "local_horizontal"
        },
        {
          "type": "origins:target_action",
          "action": {
              "type": "origins:add_velocity",
              "y": 0.3
          }
        },
        {
          "type": "origins:target_action",
          "action": {     
          "type":"origins:delay",
          "ticks":2,
          "action":
            {
            "type": "origins:execute_command",
            "command": "power grant @s hypno:playstyle/artemis/minotaur/cucked"
            }
        }
        }, 
        {
          "type": "origins:actor_action",
          "action": {
            "type": "origins:and",
            "actions": [
              {
                "type": "origins:change_resource",
                "resource": "*:*_remaining",
                "change": 0,
                "operation": "set"
              },
              {
                "type": "origins:play_sound",
                "sound": "minecraft:entity.goat.ram_impact",
                "volume": 2
              },
              {
                "type": "origins:add_velocity",
                "z": 0,
                "x": 0,
                "set": true
              }
            ]
          }
        }
      ]
    },
    "damage_condition": {
      "type": "origins:projectile",
      "inverted": true
    },
    "condition": {
      "type": "origins:resource",
      "resource": "*:*_remaining",
      "comparison": ">",
      "compare_to": 0
    }
  },
  
  "damage": {
    "type": "origins:modify_damage_dealt",
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:on_block"
        },
        {
          "type": "origins:resource",
          "resource": "*:*_remaining",
          "comparison": ">",
          "compare_to": 0
        }
      ]
    },
    "damage_condition": {
      "type": "origins:projectile",
      "inverted": true
    },
    "modifier": {
      "operation": "addition",
      "value": 3
    }
  },
  "launch":{
    "type": "origins:action_on_block_use",
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:play_sound",
          "sound": "minecraft:entity.goat.ram_impact",
          "volume": 2
        },
        {
          "type": "origins:add_velocity",
          "z": -0.4,
          "y": 0.4,
          "client": false,
          "space": "local"
        },
        {     
          "type":"origins:delay",
          "ticks":1,
          "action":{
            "type": "origins:change_resource",
            "resource": "*:*_remaining",
            "change": 0,
            "operation": "set"
          }
        }
      ]
    },
    "block_action":{
      "type": "origins:execute_command",
      "command": "particle minecraft:cloud ~ ~1 ~ 0.25 0 0.25 0.01 100"
    },
    "directions": [
        "up"
    ],
    "condition":{
      "type": "origins:resource",
      "resource": "*:*_remaining",
      "comparison": ">",
      "compare_to": 0
    }
}
}