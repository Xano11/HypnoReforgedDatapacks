                                "condition": {
                                    "type": "origins:and",
                                    "conditions": [
                                        {
                                            "type": "origins:entity_type",
                                            "entity_type": "minecraft:drowned"
                                        },
                                        {
                                            "type": "origins:command",
                                            "command": "execute if entity @e[distance=..24,tag=hooked]",
                                            "comparison": ">=",
                                            "compare_to": 1
                                        }
                                    ]
                                }
                            }
                        ]
                    }
                }
            ]
        },
        "interval": 1
    },
