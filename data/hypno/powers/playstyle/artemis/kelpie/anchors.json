{
    "name": "Anchor",
    "description": "Using your [PRIMARY ABILITY KEY], you can dash forward, putting you on fire for five seconds. Additionally, using it whilst winged will trigger your Awakening ability.",
    "type": "origins:multiple",
    "use": {
        "type": "origins:action_on_block_use",
        "block_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "summon drowned ~ ~-0.5 ~ {NoGravity:1b,Silent:1b,Invulnerable:0b,DeathLootTable:\"minecraft:empty\",NoAI:1b,CanPickUpLoot:0b,Tags:[\"anchor\"]}"
                },
                {
                    "type": "origins:execute_command",
                    "command": "power grant @e[limit=1,sort=nearest,type=drowned,tag=anchor] hypno:playstyle/artemis/kelpie/anchored"
                },
                {
                    "type": "origins:execute_command",
                    "command": "particle minecraft:soul ~ ~ ~ 0.3 0 0.3 0.05 100"
                },
                {
                    "type": "origins:execute_command",
                    "command": "playsound minecraft:entity.drowned.death_water player @a ~ ~ ~ 2 0.5"
                },
                {
                    "type": "origins:delay",
                    "ticks": 1,
                    "action": {
                        "type": "origins:execute_command",
                        "command": "execute store result score @e[limit=1,sort=nearest,type=drowned,tag=anchor] anchors run data get entity @p[tag=kelpie_anchor] UUID[0] 1"
                    }
                }
            ]
        },
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "tag @s add kelpie_anchor"
                },
                {
                    "type": "origins:change_resource",
                    "resource": "hypno:playstyle/artemis/kelpie/anchors_cooldown",
                    "change": 1200
                },
                {
                    "type": "origins:execute_command",
                    "command": "execute store result score @s anchors run data get entity @s UUID[0] 1"
                },
                {
                    "type": "origins:delay",
                    "ticks": 2,
                    "action": {
                        "type": "origins:execute_command",
                        "command": "tag @s remove kelpie_anchor"
                    }
                }
            ]
        },
        "block_condition": {
            "type": "origins:offset",
            "condition": {
                "type": "origins:block",
                "block": "minecraft:water"
            },
            "y": 1
        },
        "directions": [
            "up"
        ],
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:sneaking"
                },
                {
                    "type": "origins:resource",
                    "resource": "hypno:playstyle/artemis/kelpie/anchors_cooldown",
                    "comparison": "==",
                    "compare_to": 0
                }
            ]
        }
    },
    "cooldown": {
        "type": "origins:resource",
