{
    "name": "Wanderer of living and the dead",
    "description": "Using your [PRIMARY ABILITY KEY], you can leave your body and go into the land of the dead.",
    "type": "origins:multiple",
    "living_dead": {
        "type": "origins:resource",
        "min": 0,
        "max": 1,
        "start_value": 0,
        "hud_render": {
            "should_render": false
        }
    },
    "tp_timer": {
        "type": "origins:resource",
        "min": 0,
        "max": 40,
        "start_value": 0,
        "hud_render": {
            "should_render": false
        },
        "min_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "execute as @e[type=armor_stand,tag=marktp] if score @s marktp = @p marktp run tp @p @s"
                },
                {
                    "type": "origins:execute_command",
                    "command": "execute as @e[type=armor_stand,tag=marktp] if score @s marktp = @p marktp run kill @s"
                }
            ]
        }
    },
    "tp_timer_down": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:change_resource",
                    "resource": "hypno:playstyle/ares/yatagarasu/traveler_tp_timer",
                    "change": -1
                }
            ]
        },
        "interval": 1
    },
    "activation": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:resource",
                        "resource": "hypno:playstyle/ares/yatagarasu/traveler_living_dead",
                        "comparison": "==",
                        "compare_to": 0
                    },
                    "if_action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:execute_command",
                                "command": "power grant @s hypno:playstyle/ares/yatagarasu/dead"
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "execute store result score @s souless run data get entity @s UUID[0] 1"
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "summon wither_skeleton ~ ~ ~ {NoGravity:1b,Silent:1b,DeathLootTable:\"minecraft:empty\",NoAI:1b,CanPickUpLoot:0b,Tags:[\"souless\"]}"
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "power grant @e[limit=1,sort=nearest,type=wither_skeleton,tag=souless] hypno:playstyle/ares/yatagarasu/souless"
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "particle minecraft:soul ~ ~ ~ 0.3 0 0.3 0.05 100"
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "playsound minecraft:entity.wither.hurt player @a ~ ~ ~ 2 0.5"
                            },
                            {
                                "type": "origins:delay",
                                "ticks": 1,
                                "action": {
                                    "type": "origins:execute_command",
                                    "command": "execute store result score @e[limit=1,sort=nearest,type=wither_skeleton,tag=souless] souless run data get entity @p UUID[0] 1"
                                }
                            },
                            {
                                "type": "origins:change_resource",
                                "resource": "hypno:playstyle/ares/yatagarasu/traveler_living_dead",
                                "change": 1
                            }
                        ]
                    },
                    "else_action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:execute_command",
                                "command": "power remove @s hypno:playstyle/ares/yatagarasu/dead"
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "execute as @e[type=wither_skeleton,tag=souless] if score @s souless = @p souless run tp @p @s"
                            },
                            {
                                "type": "origins:change_resource",
                                "resource": "hypno:playstyle/ares/yatagarasu/traveler_living_dead",
                                "change": -1
                            },
                            {
                                "type": "origins:delay",
                                "ticks": 1,
                                "action": {
                                    "type": "origins:execute_command",
                                    "command": "execute as @e[type=wither_skeleton,tag=souless] if score @s souless = @p souless run kill @s"
                                }
                            }
                        ]
                    }
                }
            ]
        },
        "cooldown": 10,
        "hud_render": {
            "should_render": false
        },
        "key": {
            "key": "key.origins.secondary_active",
            "continuous": false
        }
    },
    "hidden": false
}