{
    "name":"Portaling",
    "description":"(You're in another dimension)",
    "type": "origins:multiple",
    "resource":
    {
        "type": "origins:resource",
        "min": 0,
        "max": 20,
        "start_value": 20,
        "hud_render": {
            "sprite_location": "origins:textures/gui/resource_bar.png",
            "bar_index": 2
        },
        "min_action": {
            "type": "origins:execute_command",
            "command": "power remove @s hypno:playstyle/ares/oni/slashed"
        }
    },
    "resource_use":{
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "particle minecraft:sweep_attack ~ ~1 ~ 0.3 0.3 0.3 0.1 3"
                },
                {
                    "type": "origins:execute_command",
                    "command": "playsound bettercombat:axe_slash player @a ~ ~ ~ 2 1.5"
                }
            ]
        },
        "interval": 1
    },
    "particles_and_sounds":{
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:change_resource",
                    "resource": "hypno:playstyle/ares/oni/slashed_resource",
                    "change": -1
                }
            ]
        },
        "interval": 1
    },
    "no_movement":
    {
        "type": "origins:modify_velocity",
        "modifier": {
          "value": -1,
          "operation": "multiply_total"
        },
        "axes": [
          "x",
          "y",
          "z"
        ]
    },
    "no_damage":{
        "type": "origins:modify_damage_dealt",
        "modifier": {
            "name": "No damage",
            "operation": "multiply_total",
            "value": -1
        }
    },
    "no_entity":{
        "type": "origins:prevent_entity_render"
    },
    "no_sprint":{
        "type": "origins:prevent_sprinting"
    },
    "no_collision":{
        "type": "origins:prevent_entity_collision"
    },
    "no_block_interaction":{
        "type": "origins:prevent_block_selection"
    },
    "no_block_use":{
        "type": "origins:prevent_block_use"
    },
    "prevent_item_use":{
        "type": "origins:prevent_item_use"
    },
    "hidden": false
}